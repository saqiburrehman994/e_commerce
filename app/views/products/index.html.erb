<h1>Products</h1>
<% @products.each do |product| %>
         <p>Name: <%= product.name %></p>
         <p>Description: <%= product.description %></p>
         <p>Price: <%= product.price %></p>
         <p>Stock_quantity: <%= product.stock_quantity %></p>
         <hr>
<%end%>
<%= link_to 'New Product', new_product_path %>